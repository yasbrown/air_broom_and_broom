<%= form_with model: @broomstick do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :speed %>
  <%= f.number_field :speed %>
  <%= f.label :stability %>
  <%= f.number_field :stability %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit %>
  <%= link_to "Cancel", broomstick_path(@broomstick) %>
<% end %>
